<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.themindreader"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/the_mind_reader"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.themindreader.WelcomeActivity"
            android:label="@string/the_mind_reader" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.themindreader.FirstActivity"
            android:label="@string/the_mind_reader" >
        </activity>
        <activity
            android:name="com.example.themindreader.ResultActivity"
            android:label="@string/title_activity_result" >
        </activity>
    </application>

</manifest>